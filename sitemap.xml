---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
<url>
<lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
<loc>{{site.url}}/</loc>
<changefreq>monthly</changefreq>
</url>
<url>
<lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
<loc>{{site.url}}/feed.xml</loc>
<changefreq>daily</changefreq>
<priority>0.8</priority>
</url>{% assign posts = site.data.things | sort:"created_at" | reverse %}{% for post in posts %}{% assign paragraphs = post.content | newline_to_br | split: '<br />' %}{% assign title = paragraphs.first | markdownify | strip_html | strip_newlines %}
<url>
<lastmod>{{ post.modified_at | date: "%Y-%m-%d" }}</lastmod>
<loc>{{site.url}}/been-making/#{{ post.uuid }}</loc>
<changefreq>weekly</changefreq>
<priority>0.5</priority>
</url>{% endfor %}
</urlset>